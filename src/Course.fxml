                  <DropShadow />
               </effect>
               <font>
                  <Font name="Book Antiqua" size="23.0" />
               </font>
            </Button>
            <Label layoutX="295.0" layoutY="128.0" prefHeight="54.0" prefWidth="376.0" text="Course Adding Form" textFill="#20b720">
               <font>
                  <Font name="System Bold" size="37.0" />
               </font>
            </Label>
            <Button layoutX="497.0" layoutY="388.0" minWidth="51.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="51.0" style="-fx-background-color: #331e1e;" text="EXIT" textFill="#bda3a3">
               <effect>
                  <PerspectiveTransform />
               </effect>
            </Button>
            <Button layoutX="750.0" layoutY="620.0" mnemonicParsing="false" onAction="#LoadMain" prefHeight="32.0" prefWidth="118.0" style="-fx-background-color: #aca0a0;" text="BACK">
               <effect>
                  <DropShadow />
               </effect>
               <font>
                  <Font name="Book Antiqua" size="23.0" />
               </font>
            </Button>
            <ChoiceBox fx:id="dept" layoutX="322.0" layoutY="420.0" prefHeight="38.0" prefWidth="402.0" />
            <Label layoutX="168.0" layoutY="425.0" prefHeight="28.0" prefWidth="111.0" text="Department:" textFill="#ff0909">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
