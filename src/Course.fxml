<VBox prefHeight="700.0" prefWidth="900.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Home.Controller.CourseController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="700.0" prefWidth="900.0" stylesheets="@Style.css" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="168.0" layoutY="249.0" text="Course Name:" textFill="#ff0909">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Label layoutX="171.0" layoutY="336.0" prefHeight="28.0" prefWidth="111.0" text="Course ID:" textFill="#ff0909">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <TextField fx:id="t1" layoutX="322.0" layoutY="250.0" prefHeight="26.0" prefWidth="401.0" style="-fx-background-color: #fff;" styleClass="text-field" stylesheets="@text.css">
               <effect>
                  <DropShadow />
               </effect>
            </TextField>
            <TextField fx:id="t2" layoutX="322.0" layoutY="337.0" prefHeight="26.0" prefWidth="402.0" style="-fx-background-color: #fff;" styleClass="textbox1" stylesheets="@text.css">
               <effect>
                  <DropShadow />
               </effect>
            </TextField>
            <Button layoutX="419.0" layoutY="533.0" mnemonicParsing="false" onAction="#InfoGetter" prefHeight="32.0" prefWidth="118.0" style="-fx-background-color: #aca0a0;" text="Submit">
